#include <stdio.h> 
#define MAX_SIZE 100        
int main()
{
    int arr[MAX_SIZE];
    int i, N;
    printf("Enter size of the array: ");
    scanf("%d", &N);
    printf("Enter elements in array :\n ");
    for(i=0; i<N; i++)
    {
        scanf("%d", &arr[i]);
    }

    printf("\n All negative elements in array are : ");
    for(i=0; i<N; i++)
    {
        if(arr[i] < 0)
        {
            printf("%d\t", arr[i]);
        }
    }
    return 0;
}
